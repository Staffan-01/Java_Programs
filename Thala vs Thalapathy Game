import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int s=sc.nextInt();
      if(s%3==0 && s%5==0)
        System.out.print("ThalaThalapathy");
      else if(s%3!=0 && s%5!=0)
        System.out.print(s);
      else if(s%3==0 && s%5!=0)
        System.out.print("Thala");
      else if(s%3!=0 && s%5==0)
        System.out.print("Thalapathy");
  }
}
