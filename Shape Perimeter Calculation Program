import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int s=sc.nextInt(),t=sc.nextInt(),a=sc.nextInt(),f=sc.nextInt(),n=sc.nextInt(),i=sc.nextInt(),b=sc.nextInt();
      System.out.println(4*s);
      System.out.println(2*(t+a));
      System.out.printf("%.2f\n",(3.14*2*f));
      System.out.println(n+i+b);
  }
}
