import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n],temp[]=new int[n],j=0;
		for(int i=0;i<n;arr[i++]=sc.nextInt());
		Arrays.sort(arr);
		for(int i=0;i<n-1;i++){
		    if(arr[i]!=arr[i+1]){
		        temp[j]=arr[i];
		        j++;
		    }else
		        System.out.print(arr[i]+" ");
		}
		temp[j]=arr[arr.length-1];
		for(int i=0;i<n;System.out.print(temp[i++]+" "));
	}
}
