import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int s=sc.nextInt();
		int arr[]=new int[s];
		for(int i=0;i<s;arr[i++]=sc.nextInt());
		Arrays.sort(arr);
		System.out.println("Highest speed: "+arr[s-1]);
		System.out.println("Lowest speed: "+arr[0]);
		double avg=0;
		for(int i=0;i<s;avg+=arr[i++]);
		avg/=5;
		System.out.printf("Average speed: %.1f",avg);
	}
}
