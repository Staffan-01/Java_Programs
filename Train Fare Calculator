import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int s=sc.nextInt(),t=sc.nextInt();
      double f=t*5;
      if(s<12)
        f=f*(50/100.0);
      else if(s>=60)
        f=f*(75/100.0);
      System.out.print((int)f);
    }
}
